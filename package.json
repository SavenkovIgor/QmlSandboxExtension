{
  "name": "QmlSandboxExtension",
  "displayName": "QmlSandbox",
  "description": "Qml sandbox for vscode",
  "icon": "resources/logo.png",
  "version": "3.0.0",
  "publisher": "savenkovigor",
  "engines": {
    "vscode": "^1.79.0"
  },
  "extensionDependencies": [
    "bbenoist.QML"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "QmlSandboxExtension.openQmlSandbox",
        "title": "Open Qml Sandbox",
        "icon": "$(eye)"
      },
      {
        "command": "QmlSandboxExtension.screenshotQml",
        "title": "Screenshot Qml Scene",
        "icon": "$(device-camera)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "QmlSandboxExtension.openQmlSandbox",
          "group": "navigation",
          "when": "editorLangId == qml"
        },
        {
          "command": "QmlSandboxExtension.screenshotQml",
          "group": "navigation",
          "when": "isQmlSandboxOpen"
        }
      ]
    },
    "keybindings": [
      {
        "command": "QmlSandboxExtension.openQmlSandbox",
        "key": "ctrl+shift+q",
        "mac": "cmd+shift+q",
        "when": "editorLangId == qml"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SavenkovIgor/QmlSandboxExtension"
  }
}
